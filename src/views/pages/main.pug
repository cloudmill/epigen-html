extends ../layouts/master.pug

include ../components/footer.pug
include ../components/loader.pug

//- block cookies
block grid-test

mixin nav-page-d
  nav.nav-page-d(data-aos='fade-up')
    ul.nav-page-d__list
      -
        var items = [
          {
            href: '#',
            scroll: '#products',
            title: 'Продукция Эпиген Интим',
          },
          {
            href: '#',
            scroll: '#expert',
            title: 'Экспертное мнение',
          },
          {
            href: '#',
            scroll: '#actual',
            title: 'Актуальное',
          },
          {
            href: '#',
            scroll: '#reviews',
            title: 'Отзывы',
          },
        ]
      each item, index in items
        li.nav-page-d__item(class = ((index === 0) ? 'nav-page-d__item--active' : ''))
          a.nav-page-d__link(href=item.href data-scroll=item.scroll)
            |!{item.title}

mixin nav-page-m
  nav.nav-page-d.nav-page-d--mobile
    ul.nav-page-d__list(data-aos='fade-up')
      -
        var items = [
          {
            href: '#',
            scroll: '#products',
            title: 'Продукция Эпиген Интим',
          },
          {
            href: '#',
            scroll: '#expert',
            title: 'Экспертное мнение',
          },
          {
            href: '#',
            scroll: '#actual',
            title: 'Актуальное',
          },
          {
            href: '#',
            scroll: '#reviews',
            title: 'Отзывы',
          },
        ]
      each item, index in items
        li.nav-page-d__item(class = ((index === 0) ? 'nav-page-d__item--active' : ''))
          a.category-item.category-item--light-blue.category-item--background--white(href=item.href data-scroll=item.scroll)
            |!{item.title}

mixin copyright-link
  a.copyright-link(href=attributes.href)
    .copyright-link__mark
      +copyright-svg.copyright-link__mark-svg
    .copyright-link__text
      block

